    <url desc="Support">https://github.com/PalanteJon/coop.palantetech.countylookup</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2016-03-26</releaseDate>
  <version>1.0</version>
  <develStage>stable</develStage>
  <compatibility>
